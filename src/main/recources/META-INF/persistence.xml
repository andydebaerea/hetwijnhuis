<?xml version="1.0" encoding="UTF-8"?>
<persistence version='2.0'
	xmlns='http://java.sun.com/xml/ns/persistence' xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
	xsi:schemaLocation='http://java.sun.com/xml/ns/persistence
http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd'>
	<persistence-unit name='wijnhuis'>
	<!--  <non-jta-data-source>java:/comp/env/jdbc/wijnhuis</non-jta-data-source> -->
	<class>be.hetwijnhuis.entities.Land</class>
	<class>be.hetwijnhuis.entities.Soort</class>
	<class>be.hetwijnhuis.entities.Wijn</class>
	<class>be.hetwijnhuis.entities.Bestelbon</class>
	<class>be.hetwijnhuis.valueobjects.BestelbonLijn</class>
		<properties>
			<property name='hibernate.show_sql' value='true' />
			<property name='hibernate.format_sql' value='true' />
			<property name='hibernate.use_sql_comments' value='true' />
		</properties>
	</persistence-unit>
</persistence>